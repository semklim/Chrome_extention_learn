{
  "manifest_version": 3,
  "name": "My App",
  "version": "1.0.1",
  "content_scripts": [
		{
			"js": [
				"./contentScript.js"
			],
			"matches": [
				"http://*/*",
				"https://*/*",
				"file://*/*"
			]
		
		}
	],
	"icons": { 
        "128": "./assets/icon.png" 
		},
		"action":{
			"default_title": "Windows",
    		"default_popup": "popup.html" 
		},
		"permissions": [
			"storage"
		]
}